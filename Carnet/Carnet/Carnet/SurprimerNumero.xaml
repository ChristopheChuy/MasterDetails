<Window x:Class="Carnet.SurprimerNumero"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Supprimer Numero" Height="140" Width="300"
        Icon="Icone.ico">
    <DockPanel Background="#FFE6E6E6">
        <StackPanel DockPanel.Dock="Top">
            <TextBlock Text="Numero : " Margin="25,0,0,0" HorizontalAlignment="Center"/>
            <ComboBox Margin="30,10" Name="ListeNumero" SelectedIndex="0"></ComboBox>
        </StackPanel>
        <WrapPanel DockPanel.Dock="Bottom"  VerticalAlignment="Bottom" HorizontalAlignment="Center">
            <Button Margin=" 8" Click="fermer" >Annuler</Button>
            <Button Margin=" 8" Click="suprimer" >Supprimer</Button>
        </WrapPanel>
    </DockPanel>
</Window>
